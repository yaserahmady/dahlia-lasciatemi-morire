#. ID 1, Page 1, Line 6, Pos (0,0)
#. ID 1, Page 3, Line 65, Pos (0,0)
#. ID 1, Page 3, Line 516, Pos (0,0)
msgid "\\> ………………。"
msgstr ""

#. ID 1, Page 1, Line 10, Pos (0,0)
msgid "\\>　　　　　　　やはり私を殺しに来たか、ダリヤ……　"
msgstr ""

#. ID 1, Page 1, Line 11, Pos (0,0)
msgid "\\>　　　　　　　ずっと、そんな予感はしていた……"
msgstr ""

#. ID 1, Page 1, Line 13, Pos (0,0)
msgid ""
"\\>　　　　　　　だが、私はまだ死ぬわけにはいかない。\n"
"\\>　　　　　　　私が、地球再生のプロジェクトを\n"
"\\>　　　　　　　完遂させるその日まで。"
msgstr ""

#. ID 1, Page 1, Line 17, Pos (0,0)
msgid ""
"\\>　　　　　　　なぜ、お前が私を\n"
"\\>　　　　　　　殺したいのか知らないが、\n"
"\\>　　　　　　　言葉を持たない今のお前では――"
msgstr ""

#. ID 1, Page 1, Line 20, Pos (0,0)
msgid ""
"\\>　　　　　　　いや、たとえ声が出ていたとしても、\n"
"\\>　　　　　　　語らうことによってお前の気を\n"
"\\>　　　　　　　変えさせることは出来ないだろう。"
msgstr ""

#. ID 1, Page 1, Line 24, Pos (0,0)
msgid "\\>　　　　　　　もしここで、私がお前を殺したら――"
msgstr ""

#. ID 1, Page 1, Line 25, Pos (0,0)
msgid ""
"\\>　　　　　　　プロジェクト完成の後、\n"
"\\>　　　　　　　私はお前の後を追おう……"
msgstr ""

#. ID 1, Page 1, Line 28, Pos (0,0)
msgid ""
"\\>　　　　　　　だが今は、\n"
"\\>　　　　　　　止まるわけにはいかないのだ……"
msgstr ""

#. ID 1, Page 1, Line 34, Pos (0,0)
msgid ""
"\\> お久しぶりね、フォルドゥン。\n"
"\\> 元気にしていたかしら。"
msgstr ""

#. ID 1, Page 1, Line 38, Pos (0,0)
msgid ""
"\\>　　　　　　　ソーニャ、か……\n"
"\\>　　　　　　　まさかお前まで私を？"
msgstr ""

#. ID 1, Page 1, Line 42, Pos (0,0)
msgid ""
"\\> 察しの通りよ。\n"
"\\> あなたをプロジェクトから\n"
"\\> 退かせるためには、\n"
"\\> 殺すしか手段はないと悟ったから。"
msgstr ""

#. ID 1, Page 1, Line 46, Pos (0,0)
msgid ""
"\\> あれから……\n"
"\\> 私はあなたを止めることだけを\n"
"\\> 考えて生きてきたわ。"
msgstr ""

#. ID 1, Page 1, Line 49, Pos (0,0)
msgid ""
"\\> そのためには、\n"
"\\> どんな手段も辞さなかった。\n"
"\\> たとえ自ら志願して\n"
"\\> 生体兵器となろうともね。"
msgstr ""

#. ID 1, Page 1, Line 55, Pos (0,0)
msgid "\\>　　　　　　　フフ……"
msgstr ""

#. ID 1, Page 1, Line 56, Pos (0,0)
msgid ""
"\\>　　　　　　　ククク……\n"
"\\>　　　　　　　ハハハハハハハハ！"
msgstr ""

#. ID 1, Page 1, Line 60, Pos (0,0)
msgid "\\> 私が何か、おかしなことでも？"
msgstr ""

#. ID 1, Page 1, Line 63, Pos (0,0)
msgid ""
"\\>　　　　　　　おかしいさ。\n"
"\\>　　　　　　　このシチュエーションが、だ。"
msgstr ""

#. ID 1, Page 1, Line 65, Pos (0,0)
msgid ""
"\\>　　　　　　　私と、ダリヤと、お前がここにいて、\n"
"\\>　　　　　　　そしてダリヤがお前について、\n"
"\\>　　　　　　　私を殺そうというのだ。"
msgstr ""

#. ID 1, Page 1, Line 68, Pos (0,0)
msgid ""
"\\>　　　　　　　なんと皮肉な……\n"
"\\>　　　　　　　実に喜劇的ですら\n"
"\\>　　　　　　　ある話だと思ったのだ。"
msgstr ""

#. ID 1, Page 1, Line 73, Pos (0,0)
msgid "\\> ……この子が、何か？"
msgstr ""

#. ID 1, Page 1, Line 76, Pos (0,0)
msgid ""
"\\>　　　　　　　分からないのか？\n"
"\\>　　　　　　　ダリヤは私の娘だ。"
msgstr ""

#. ID 1, Page 1, Line 78, Pos (0,0)
msgid "\\>　　　　　　　つまり……言わずとも知れよう？"
msgstr ""

#. ID 1, Page 1, Line 81, Pos (0,0)
msgid "\\> ……！"
msgstr ""

#. ID 1, Page 1, Line 82, Pos (0,0)
msgid "\\> そんな、まさか……"
msgstr ""

#. ID 1, Page 1, Line 85, Pos (0,0)
msgid ""
"\\>　　　　　　　私も、お前も、若かった。\n"
"\\>　　　　　　　いや、幼かったというべきか。"
msgstr ""

#. ID 1, Page 1, Line 87, Pos (0,0)
msgid ""
"\\>　　　　　　　もはや語るまい。\n"
"\\>　　　　　　　ここに至っては、\n"
"\\>　　　　　　　そのような事実も既に無意味だ。"
msgstr ""

#. ID 1, Page 1, Line 101, Pos (0,0)
msgid ""
"\\>　　　　　　　さあ、\n"
"\\>　　　　　　　やるというなら全力で来るがいい。"
msgstr ""

#. ID 1, Page 1, Line 103, Pos (0,0)
msgid ""
"\\>　　　　　　　生体兵器として、\n"
"\\>　　　　　　　自ら改良を重ねたこの身体、\n"
"\\>　　　　　　　生半可では朽ちん。"
msgstr ""

#. ID 1, Page 1, Line 108, Pos (0,0)
msgid ""
"\\>　　　　　　　私を殺せ！\\<\\!\n"
"\\>　　　　　　　そして生きろ！\\<\\!\n"
"\\>　　　　　　　それがお前の生きる道というならば！"
msgstr ""

#. ID 1, Page 3, Line 2, Pos (0,0)
msgid "\\>　　　　　　　やはり……"
msgstr ""

#. ID 1, Page 3, Line 3, Pos (0,0)
msgid "\\>　　　　　　　こう、なったか……"
msgstr ""

#. ID 1, Page 3, Line 5, Pos (0,0)
msgid "\\>　　　　　　　フフ……お前は……"
msgstr ""

#. ID 1, Page 3, Line 6, Pos (0,0)
msgid ""
"\\>　　　　　　　最後の最後まで、\n"
"\\>　　　　　　　分からない子だった……"
msgstr ""

#. ID 1, Page 3, Line 8, Pos (0,0)
msgid ""
"\\>　　　　　　　何を考えているのかも、\n"
"\\>　　　　　　　何が欲しいのかも、\n"
"\\>　　　　　　　どう愛したらいいのかも……"
msgstr ""

#. ID 1, Page 3, Line 12, Pos (0,0)
msgid ""
"\\>　　　　　　　私には、理解者と呼べるものはなく、\n"
"\\>　　　　　　　お前もまた、誰からも理解されない……"
msgstr ""

#. ID 1, Page 3, Line 14, Pos (0,0)
msgid ""
"\\>　　　　　　　思ったのだ……\n"
"\\>　　　　　　　私の孤独を癒してくれるのは、\n"
"\\>　　　　　　　お前だけだと……"
msgstr ""

#. ID 1, Page 3, Line 18, Pos (0,0)
msgid ""
"\\>　　　　　　　だが、違う。\n"
"\\>　　　　　　　お前は、理解されることを拒絶する。"
msgstr ""

#. ID 1, Page 3, Line 20, Pos (0,0)
msgid ""
"\\>　　　　　　　この世の中でも、\n"
"\\>　　　　　　　とりわけ異質な感性……"
msgstr ""

#. ID 1, Page 3, Line 22, Pos (0,0)
msgid ""
"\\>　　　　　　　私では、\n"
"\\>　　　　　　　想像もつかない人間だ、お前は……"
msgstr ""

#. ID 1, Page 3, Line 25, Pos (0,0)
msgid ""
"\\>　　　　　　　それにしても、\n"
"\\>　　　　　　　自ら世界の未来を閉ざすとは……"
msgstr ""

#. ID 1, Page 3, Line 27, Pos (0,0)
msgid ""
"\\>　　　　　　　私がなくては、\n"
"\\>　　　　　　　地球再生の道は途絶える。"
msgstr ""

#. ID 1, Page 3, Line 29, Pos (0,0)
msgid ""
"\\>　　　　　　　人類が進む道は、\n"
"\\>　　　　　　　必然としての滅びだ……"
msgstr ""

#. ID 1, Page 3, Line 31, Pos (0,0)
msgid ""
"\\>　　　　　　　まあ、それも、\n"
"\\>　　　　　　　人間の選択のひとつか……"
msgstr ""

#. ID 1, Page 3, Line 34, Pos (0,0)
msgid ""
"\\>　　　　　　　……せめて最期くらい、\n"
"\\>　　　　　　　お前の、声が聞きたかった……"
msgstr ""

#. ID 1, Page 3, Line 36, Pos (0,0)
msgid "\\>　　　　　　　ダリヤ……"
msgstr ""

#. ID 1, Page 3, Line 51, Pos (0,0)
msgid "\\>　　　　　　　勝っ……ちまったのか……？"
msgstr ""

#. ID 1, Page 3, Line 52, Pos (0,0)
msgid "\\>　　　　　　　意外とあっさりしたモンだな……"
msgstr ""

#. ID 1, Page 3, Line 55, Pos (0,0)
#. ID 1, Page 3, Line 83, Pos (0,0)
#. ID 1, Page 3, Line 105, Pos (0,0)
#. ID 1, Page 3, Line 130, Pos (0,0)
#. ID 1, Page 3, Line 140, Pos (0,0)
#. ID 1, Page 3, Line 209, Pos (0,0)
#. ID 1, Page 3, Line 234, Pos (0,0)
#. ID 1, Page 3, Line 264, Pos (0,0)
#. ID 1, Page 3, Line 338, Pos (0,0)
#. ID 1, Page 3, Line 514, Pos (0,0)
#. ID 1, Page 5, Line 23, Pos (0,0)
msgid "\\> …………。"
msgstr ""

#. ID 1, Page 3, Line 58, Pos (0,0)
msgid "\\>　　　　　　　ん、この写真は？"
msgstr ""

#. ID 1, Page 3, Line 60, Pos (0,0)
msgid "\\>　　　　　　　これ、お前か、ダリヤ……？"
msgstr ""

#. ID 1, Page 3, Line 61, Pos (0,0)
msgid ""
"\\>　　　　　　　それとフォルドゥン……\n"
"\\>　　　　　　　やっぱりそうか、親子だったのか……"
msgstr ""

#. ID 1, Page 3, Line 68, Pos (0,0)
msgid "\\>　　　　　　　なんで、自分の父親を殺そうと……"
msgstr ""

#. ID 1, Page 3, Line 69, Pos (0,0)
msgid "\\>　　　　　　　いや、いいさ。聞かねえよ。"
msgstr ""

#. ID 1, Page 3, Line 71, Pos (0,0)
msgid "\\>　　　　　　　あー、その、なんだ。"
msgstr ""

#. ID 1, Page 3, Line 72, Pos (0,0)
msgid ""
"\\>　　　　　　　俺は、賞金が手に入ったとしても\n"
"\\>　　　　　　　ハンターをやめる気はねえ。"
msgstr ""

#. ID 1, Page 3, Line 74, Pos (0,0)
msgid ""
"\\>　　　　　　　で、おめえはどうなんだ……\n"
"\\>　　　　　　　行くところとか、ねえんだろ？"
msgstr ""

#. ID 1, Page 3, Line 77, Pos (0,0)
msgid "\\>　　　　　　　……俺と一緒に来いよ。"
msgstr ""

#. ID 1, Page 3, Line 78, Pos (0,0)
msgid ""
"\\>　　　　　　　まだしばらくは、\n"
"\\>　　　　　　　俺たちゃ“相棒”だ。"
msgstr ""

#. ID 1, Page 3, Line 80, Pos (0,0)
msgid "\\>　　　　　　　……それでどうだ？"
msgstr ""

#. ID 1, Page 3, Line 85, Pos (0,0)
msgid "\\S[4] …………ぁ……あ……ぅ………"
msgstr ""

#. ID 1, Page 3, Line 88, Pos (0,0)
msgid ""
"\\>　　　　　　　あー、分かった！\n"
"\\>　　　　　　　分かったから無理はすんな！"
msgstr ""

#. ID 1, Page 3, Line 91, Pos (0,0)
msgid ""
"\\>　　　　　　　そんじゃ、めんたまの飛び出るような\n"
"\\>　　　　　　　賞金をいただいて、それから王都の\n"
"\\>　　　　　　　一番高いメシ屋で何か食おうぜ。"
msgstr ""

#. ID 1, Page 3, Line 96, Pos (0,0)
#. ID 1, Page 3, Line 281, Pos (0,0)
#. ID 1, Page 3, Line 451, Pos (0,0)
msgid "\\>\\C[1] (nod)"
msgstr ""

#. ID 1, Page 3, Line 100, Pos (0,0)
msgid "\\>　　　　　　　……アンタの笑った顔、初めて見たぜ。"
msgstr ""

#. ID 1, Page 3, Line 101, Pos (0,0)
msgid ""
"\\>　　　　　　　なんつーか、その……\n"
"\\>　　　　　　　可愛い、じゃねえか……"
msgstr ""

#. ID 1, Page 3, Line 108, Pos (0,0)
msgid ""
"\\>　　　　　　　って、馬鹿野郎！\n"
"\\>　　　　　　　言ってみただけだ！"
msgstr ""

#. ID 1, Page 3, Line 110, Pos (0,0)
msgid ""
"\\>　　　　　　　あー、ほら、もう行こうぜ！\n"
"\\>　　　　　　　ダリヤ！"
msgstr ""

#. ID 1, Page 3, Line 124, Pos (0,0)
msgid "\\>　　　　　　　倒したか。"
msgstr ""

#. ID 1, Page 3, Line 125, Pos (0,0)
msgid ""
"\\>　　　　　　　やはり生体兵器の\n"
"\\>　　　　　　　生みの親だけあって、\n"
"\\>　　　　　　　生命力は凄まじいものがあったな。"
msgstr ""

#. ID 1, Page 3, Line 133, Pos (0,0)
msgid ""
"\\>　　　　　　　しかし、この男を倒すことが、\n"
"\\>　　　　　　　私の考える罪の浄化ではない。"
msgstr ""

#. ID 1, Page 3, Line 135, Pos (0,0)
msgid "\\>　　　　　　　この辺りにあるはずだが……"
msgstr ""

#. ID 1, Page 3, Line 136, Pos (0,0)
msgid ""
"\\>　　　　　　　ところで、どうして君は、\n"
"\\>　　　　　　　自分の父親を手にかけたのか……？"
msgstr ""

#. ID 1, Page 3, Line 143, Pos (0,0)
msgid ""
"\\>　　　　　　　……いや、すまない。\n"
"\\>　　　　　　　たとえ君に声があろうとも、\n"
"\\>　　　　　　　答えられたものではないな。"
msgstr ""

#. ID 1, Page 3, Line 147, Pos (0,0)
msgid "\\>　　　　　　　……ああ、これだ。"
msgstr ""

#. ID 1, Page 3, Line 148, Pos (0,0)
msgid ""
"\\>　　　　　　　これが、\n"
"\\>　　　　　　　私の求める真理というべきもの。"
msgstr ""

#. ID 1, Page 3, Line 151, Pos (0,0)
msgid "\\>　　　　　　　これが何だか分かるかね、ダリヤ君？"
msgstr ""

#. ID 1, Page 3, Line 152, Pos (0,0)
msgid ""
"\\>　　　　　　　統一政府が多額の賞金をかけてでも\n"
"\\>　　　　　　　求めてやまなかったもの――"
msgstr ""

#. ID 1, Page 3, Line 155, Pos (0,0)
msgid ""
"\\>　　　　　　　たった１発の弾頭で、\n"
"\\>　　　　　　　地上の３分の１の生物を消去する、\n"
"\\>　　　　　　　歴史上最高の威力を持つ\n"
"\\>　　　　　　　ミサイルの発射スイッチだ。"
msgstr ""

#. ID 1, Page 3, Line 161, Pos (0,0)
#. ID 1, Page 3, Line 415, Pos (0,0)
#. ID 1, Page 3, Line 523, Pos (0,0)
msgid "\\> ！"
msgstr ""

#. ID 1, Page 3, Line 164, Pos (0,0)
msgid ""
"\\>　　　　　　　世界のために、\n"
"\\>　　　　　　　人類を消去しようとする試行――"
msgstr ""

#. ID 1, Page 3, Line 166, Pos (0,0)
msgid ""
"\\>　　　　　　　それこそ、\n"
"\\>　　　　　　　いや、それ以外の浄化など、\n"
"\\>　　　　　　　存在しようものか。"
msgstr ""

#. ID 1, Page 3, Line 170, Pos (0,0)
msgid ""
"\\>　　　　　　　さらば、人よ。\\<\\!\n"
"\\>　　　　　　　未来の地球に光あらんことを！"
msgstr ""

#. ID 1, Page 3, Line 179, Pos (0,0)
msgid "\\>　　　　　　　ぐ……"
msgstr ""

#. ID 1, Page 3, Line 181, Pos (0,0)
msgid "\\>　　　　　　　……フ。"
msgstr ""

#. ID 1, Page 3, Line 182, Pos (0,0)
msgid "\\>　　　　　　　……ククク。"
msgstr ""

#. ID 1, Page 3, Line 183, Pos (0,0)
msgid "\\>　　　　　　　それでよいのだ……"
msgstr ""

#. ID 1, Page 3, Line 185, Pos (0,0)
msgid ""
"\\>　　　　　　　……これが、\n"
"\\>　　　　　　　私の求める最高の死、\n"
"\\>　　　　　　　私という存在の浄化というものだ。"
msgstr ""

#. ID 1, Page 3, Line 188, Pos (0,0)
msgid ""
"\\>　　　　　　　世界を滅ぼそうとする一人の男と、\n"
"\\>　　　　　　　世界を存続させんとする一人の少女……"
msgstr ""

#. ID 1, Page 3, Line 190, Pos (0,0)
msgid ""
"\\>　　　　　　　人類の代表でもない二者が、\n"
"\\>　　　　　　　一瞬の判断で世界の命運を左右する。"
msgstr ""

#. ID 1, Page 3, Line 193, Pos (0,0)
msgid ""
"\\>　　　　　　　わ、私は……満足している。\n"
"\\>　　　　　　　君の行動に。決断に……"
msgstr ""

#. ID 1, Page 3, Line 196, Pos (0,0)
msgid ""
"\\>　　　　　　　千の書をひもとき、\n"
"\\>　　　　　　　万の語を尽くすよりも、\n"
"\\>　　　　　　　刹那の迷いなき判断が\n"
"\\>　　　　　　　世界を正しく導くのだ……"
msgstr ""

#. ID 1, Page 3, Line 201, Pos (0,0)
msgid ""
"\\>　　　　　　　生きろ……\\<\\!\n"
"\\>　　　　　　　その意志のある限り……"
msgstr ""

#. ID 1, Page 3, Line 203, Pos (0,0)
msgid ""
"\\>　　　　　　　屈するな……\n"
"\\>　　　　　　　君なら、真実を、その手に……"
msgstr ""

#. ID 1, Page 3, Line 221, Pos (0,0)
msgid "\\>　　　　　　　はぁ……はぁ…………"
msgstr ""

#. ID 1, Page 3, Line 223, Pos (0,0)
msgid ""
"\\>　　　　　　　た、倒したの……？\n"
"\\>　　　　　　　私たちが……？"
msgstr ""

#. ID 1, Page 3, Line 226, Pos (0,0)
msgid ""
"\\>　　　　　　　……でも、フォルドゥンって、\n"
"\\>　　　　　　　ダリヤちゃんの\n"
"\\>　　　　　　　お父さんだったんだね……"
msgstr ""

#. ID 1, Page 3, Line 229, Pos (0,0)
msgid "\\>　　　　　　　どうして……"
msgstr ""

#. ID 1, Page 3, Line 230, Pos (0,0)
msgid ""
"\\>　　　　　　　いや、ダリヤちゃんにも、\n"
"\\>　　　　　　　いろいろあるんだよね……"
msgstr ""

#. ID 1, Page 3, Line 238, Pos (0,0)
msgid ""
"\\>　　　　　　　……それにしても、\n"
"\\>　　　　　　　本当にあんな額の賞金が\n"
"\\>　　　　　　　もらえるのかな？"
msgstr ""

#. ID 1, Page 3, Line 241, Pos (0,0)
msgid ""
"\\>　　　　　　　もらえたら、\n"
"\\>　　　　　　　もし、ちゃんともらえたらね……"
msgstr ""

#. ID 1, Page 3, Line 243, Pos (0,0)
msgid "\\>　　　　　　　あたしはみんなのために使おうと思う。"
msgstr ""

#. ID 1, Page 3, Line 244, Pos (0,0)
msgid ""
"\\>　　　　　　　みんなって言っても限界はあるから、\n"
"\\>　　　　　　　一番、人のためになるような、ね。"
msgstr ""

#. ID 1, Page 3, Line 247, Pos (0,0)
msgid ""
"\\>　　　　　　　親に捨てられた子供たちを集めて、\n"
"\\>　　　　　　　ちゃんとした生活をさせて、\n"
"\\>　　　　　　　ちゃんとしたことを教えるの。"
msgstr ""

#. ID 1, Page 3, Line 250, Pos (0,0)
msgid ""
"\\>　　　　　　　そうしたらたぶん、\n"
"\\>　　　　　　　男の子は他の人のものを奪わない、\n"
"\\>　　　　　　　女の子も自分の身体を売ったりしない、\n"
"\\>　　　　　　　そんな人になれるんじゃないかな。"
msgstr ""

#. ID 1, Page 3, Line 254, Pos (0,0)
msgid ""
"\\>　　　　　　　もちろんさ、\n"
"\\>　　　　　　　そんな上手くばっかりは\n"
"\\>　　　　　　　いかないと思うけど、"
msgstr ""

#. ID 1, Page 3, Line 257, Pos (0,0)
msgid "\\>　　　　　　　それでもあたしは――"
msgstr ""

#. ID 1, Page 3, Line 259, Pos (0,0)
msgid ""
"\\>　　　　　　　あたしはそうしたい。\n"
"\\>　　　　　　　その意志が、\n"
"\\>　　　　　　　あたしの生命そのものだから。"
msgstr ""

#. ID 1, Page 3, Line 268, Pos (0,0)
msgid ""
"\\>　　　　　　　え……何？\n"
"\\>　　　　　　　手を……？"
msgstr ""

#. ID 1, Page 3, Line 270, Pos (0,0)
msgid ""
"\\>　　　　　　　あ、字を書く……？\n"
"\\>　　　　　　　うん、どうしたの？"
msgstr ""

#. ID 1, Page 3, Line 273, Pos (0,0)
#. ID 1, Page 3, Line 304, Pos (0,0)
#. ID 1, Page 3, Line 479, Pos (0,0)
msgid "\\>　　　　　　　…………。"
msgstr ""

#. ID 1, Page 3, Line 275, Pos (0,0)
msgid "\\>　　　　　　　………………。"
msgstr ""

#. ID 1, Page 3, Line 277, Pos (0,0)
msgid "\\>　　　　　　　え、ウソ……？"
msgstr ""

#. ID 1, Page 3, Line 278, Pos (0,0)
msgid "\\>　　　　　　　ダリヤちゃんも、あたしと一緒に……？"
msgstr ""

#. ID 1, Page 3, Line 285, Pos (0,0)
msgid "\\>　　　　　　　……ああ、神様！"
msgstr ""

#. ID 1, Page 3, Line 286, Pos (0,0)
msgid ""
"\\>　　　　　　　なんて言ったらいいのかな……\n"
"\\>　　　　　　　なんかもう、言葉に詰まっちゃって……"
msgstr ""

#. ID 1, Page 3, Line 289, Pos (0,0)
msgid "\\>　　　　　　　……ありがとう。"
msgstr ""

#. ID 1, Page 3, Line 291, Pos (0,0)
msgid ""
"\\>　　　　　　　ダリヤちゃんがいてくれれば、\n"
"\\>　　　　　　　なんでもうまくいくような気がする。"
msgstr ""

#. ID 1, Page 3, Line 293, Pos (0,0)
msgid ""
"\\>　　　　　　　最初に助けてもらった時から\n"
"\\>　　　　　　　そうだけど――"
msgstr ""

#. ID 1, Page 3, Line 296, Pos (0,0)
msgid "\\>　　　　　　　あたしはダリヤちゃんを信じてるから。"
msgstr ""

#. ID 1, Page 3, Line 297, Pos (0,0)
msgid ""
"\\>　　　　　　　たとえ何があっても、\n"
"\\>　　　　　　　あたしは自分の命を、\n"
"\\>　　　　　　　ダリヤちゃんに預けられる。"
msgstr ""

#. ID 1, Page 3, Line 300, Pos (0,0)
msgid ""
"\\>　　　　　　　たとえダリヤに殺されたとしても、\n"
"\\>　　　　　　　あたしはダリヤちゃんを恨まない。\n"
"\\>　　　　　　　それはきっと、間違ってないから。"
msgstr ""

#. ID 1, Page 3, Line 306, Pos (0,0)
msgid "\\>　　　　　　　って、何言ってるんだろうね、あたし。"
msgstr ""

#. ID 1, Page 3, Line 307, Pos (0,0)
msgid ""
"\\>　　　　　　　でもそういう気持ちなんだってこと、\n"
"\\>　　　　　　　伝えたかったから。ね？"
msgstr ""

#. ID 1, Page 3, Line 310, Pos (0,0)
msgid "\\>　　　　　　　……さて、と！"
msgstr ""

#. ID 1, Page 3, Line 311, Pos (0,0)
msgid ""
"\\>　　　　　　　それなら、これからが本番だから！\n"
"\\>　　　　　　　気を引き締めないとね！"
msgstr ""

#. ID 1, Page 3, Line 314, Pos (0,0)
msgid "\\>　　　　　　　これからもよろしくね、ダリヤちゃん！"
msgstr ""

#. ID 1, Page 3, Line 327, Pos (0,0)
msgid ""
"\\>　　　　　　　ふう。\n"
"\\>　　　　　　　手こずったけど、\n"
"\\>　　　　　　　やっとセンターを取り戻したわ。"
msgstr ""

#. ID 1, Page 3, Line 330, Pos (0,0)
msgid ""
"\\>　　　　　　　しかし哀れな男ね。\n"
"\\>　　　　　　　自分がいなければ地球の未来は\n"
"\\>　　　　　　　ないという思い込みもそうだけど。"
msgstr ""

#. ID 1, Page 3, Line 333, Pos (0,0)
msgid ""
"\\>　　　　　　　誰にも理解されない孤独を\n"
"\\>　　　　　　　自分の娘に癒してもらおうとして、\n"
"\\>　　　　　　　仕舞いにはその娘に殺されるなんて。"
msgstr ""

#. ID 1, Page 3, Line 342, Pos (0,0)
msgid "\\>　　　　　　　……ちょっと独り言を言うわ。"
msgstr ""

#. ID 1, Page 3, Line 343, Pos (0,0)
msgid ""
"\\>　　　　　　　私は過去、フォルドゥンの助手だった。\n"
"\\>　　　　　　　統一政府の前身、\n"
"\\>　　　　　　　ケテル党に属する研究者だったの。"
msgstr ""

#. ID 1, Page 3, Line 346, Pos (0,0)
msgid ""
"\\>　　　　　　　それは１５年前のことね。\n"
"\\>　　　　　　　つまり、“大崩壊”以前のこと。\n"
"\\>　　　　　　　まだ、大地はこれほど\n"
"\\>　　　　　　　汚染されていなかった時代よ。"
msgstr ""

#. ID 1, Page 3, Line 351, Pos (0,0)
msgid ""
"\\>　　　　　　　今ほど末期的ではないにせよ、\n"
"\\>　　　　　　　その頃既に地球人類の\n"
"\\>　　　　　　　終焉は囁かれていた。"
msgstr ""

#. ID 1, Page 3, Line 354, Pos (0,0)
msgid ""
"\\>　　　　　　　それを回復して、\n"
"\\>　　　　　　　その功績によって統一政府を\n"
"\\>　　　　　　　作り上げようとケテル党は考えていた。"
msgstr ""

#. ID 1, Page 3, Line 357, Pos (0,0)
msgid ""
"\\>　　　　　　　そしてこの地に\n"
"\\>　　　　　　　『地球環境監視センター』を築き、\n"
"\\>　　　　　　　地球環境を回復させるプロジェクトを、\n"
"\\>　　　　　　　私たちは進めてきたわ。"
msgstr ""

#. ID 1, Page 3, Line 362, Pos (0,0)
msgid ""
"\\>　　　　　　　そのプロジェクトの\n"
"\\>　　　　　　　責任者はフォルドゥン。　　　"
msgstr ""

#. ID 1, Page 3, Line 364, Pos (0,0)
msgid ""
"\\>　　　　　　　彼の知能は天才的で、\n"
"\\>　　　　　　　プロジェクトのほとんどは\n"
"\\>　　　　　　　彼の力で押し進められたものなのよ。"
msgstr ""

#. ID 1, Page 3, Line 367, Pos (0,0)
msgid ""
"\\>　　　　　　　それだけに、フォルドゥンは\n"
"\\>　　　　　　　プロジェクトに自信を持っていた。\n"
"\\>　　　　　　　だから、これを中止するなんて、\n"
"\\>　　　　　　　考えもしなかった。"
msgstr ""

#. ID 1, Page 3, Line 372, Pos (0,0)
msgid ""
"\\>　　　　　　　そう、本当はこのプロジェクトは\n"
"\\>　　　　　　　中止しなければ\n"
"\\>　　　　　　　ならないものだったのよ。"
msgstr ""

#. ID 1, Page 3, Line 375, Pos (0,0)
msgid ""
"\\>　　　　　　　失敗した際のリスクは、\n"
"\\>　　　　　　　逆に地球環境を完膚なきまでに\n"
"\\>　　　　　　　破壊してしまうものだったから。"
msgstr ""

#. ID 1, Page 3, Line 379, Pos (0,0)
msgid ""
"\\>　　　　　　　フォルドゥンは、\n"
"\\>　　　　　　　おそらく自分のプライドから、\n"
"\\>　　　　　　　そのリスクについて\n"
"\\>　　　　　　　誰にも説明しなかった。"
msgstr ""

#. ID 1, Page 3, Line 383, Pos (0,0)
msgid "\\>　　　　　　　そして起こったのが、“大崩壊”……"
msgstr ""

#. ID 1, Page 3, Line 385, Pos (0,0)
msgid ""
"\\>　　　　　　　プロジェクトに\n"
"\\>　　　　　　　失敗したフォルドゥンは、\n"
"\\>　　　　　　　糾弾を逃れるため、\n"
"\\>　　　　　　　関係者を暗殺した末に逃走した。"
msgstr ""

#. ID 1, Page 3, Line 389, Pos (0,0)
msgid ""
"\\>　　　　　　　ケテル党は内部抗争から指導部が一新。\n"
"\\>　　　　　　　体制を変更し、以前から\n"
"\\>　　　　　　　手を結んでいた軍部と結託し、\n"
"\\>　　　　　　　武力によって統一政府を作り上げた。"
msgstr ""

#. ID 1, Page 3, Line 394, Pos (0,0)
msgid ""
"\\>　　　　　　　罪滅ぼしのつもり\n"
"\\>　　　　　　　だったのかも知れないけど、\n"
"\\>　　　　　　　フォルドゥンは再びここに戻り、\n"
"\\>　　　　　　　地球再生計画をたった一人で続けた。"
msgstr ""

#. ID 1, Page 3, Line 398, Pos (0,0)
msgid "\\>　　　　　　　でも、彼は分かっていなかった。"
msgstr ""

#. ID 1, Page 3, Line 399, Pos (0,0)
msgid ""
"\\>　　　　　　　もはや、彼の力ではそれ以上のことは\n"
"\\>　　　　　　　不可能であったということを。"
msgstr ""

#. ID 1, Page 3, Line 401, Pos (0,0)
msgid ""
"\\>　　　　　　　この仕事の続きを完遂出来るのは、\n"
"\\>　　　　　　　地球上にたった一人、\n"
"\\>　　　　　　　私しかいないのよ。"
msgstr ""

#. ID 1, Page 3, Line 405, Pos (0,0)
msgid ""
"\\>　　　　　　　ダリヤ……\n"
"\\>　　　　　　　あなたも私の研究を手伝わない？\n"
"\\>　　　　　　　あなたは、平凡な頭脳の\n"
"\\>　　　　　　　持ち主のはずがないから。"
msgstr ""

#. ID 1, Page 3, Line 409, Pos (0,0)
msgid "\\>　　　　　　　だってあなた……"
msgstr ""

#. ID 1, Page 3, Line 411, Pos (0,0)
msgid ""
"\\>　　　　　　　彼、フォルドゥンと、\n"
"\\>　　　　　　　私の間に出来た子だから。"
msgstr ""

#. ID 1, Page 3, Line 419, Pos (0,0)
msgid ""
"\\>　　　　　　　……知らなかったのよ。\n"
"\\>　　　　　　　８ヶ月で生み捨てた\n"
"\\>　　　　　　　つもりだった未熟児を、\n"
"\\>　　　　　　　彼が秘密裏に育て上げていたなんて。"
msgstr ""

#. ID 1, Page 3, Line 423, Pos (0,0)
msgid ""
"\\>　　　　　　　あの人は、プロジェクトのリスクを\n"
"\\>　　　　　　　言わなかっただけじゃなく、\n"
"\\>　　　　　　　そんなことすら教えていなかった。"
msgstr ""

#. ID 1, Page 3, Line 426, Pos (0,0)
msgid "\\>　　　　　　　……そういう男だったのよ。"
msgstr ""

#. ID 1, Page 3, Line 429, Pos (0,0)
#. Contains choice at line 3 (2 options)
msgid ""
"\\>\\C[2]　　　　　　　†\\C[0]　ねえ、ここに残って、私と一緒に\n"
"\\>　　　　　　　　　プロジェクトを再興しない？\\<\\.\\.\\."
msgstr ""

#. ID 1, Page 3, Line 431, Pos (0,0)
#. Choice (2 options, embedded in a message)
msgid ""
"\\>\\C[1]                    nod\n"
"\\>\\C[2]                    殺す"
msgstr ""

#. ID 1, Page 3, Line 434, Pos (0,0)
msgid "\\>　　　　　　　……なんて言ったらいいのかしら。"
msgstr ""

#. ID 1, Page 3, Line 435, Pos (0,0)
msgid ""
"\\>　　　　　　　私には、母親という意識もなければ、\n"
"\\>　　　　　　　その資格さえあるか\n"
"\\>　　　　　　　どうか分からないのに。"
msgstr ""

#. ID 1, Page 3, Line 439, Pos (0,0)
msgid ""
"\\>　　　　　　　えーと、こういう時母親は\n"
"\\>　　　　　　　どうすればいいのかしら。"
msgstr ""

#. ID 1, Page 3, Line 441, Pos (0,0)
msgid ""
"\\>　　　　　　　ねえ、その、私は、ほら、\n"
"\\>　　　　　　　親に可愛がってもらった\n"
"\\>　　　　　　　覚えがないから……"
msgstr ""

#. ID 1, Page 3, Line 444, Pos (0,0)
msgid "\\>　　　　　　　ああもう、困るじゃない。"
msgstr ""

#. ID 1, Page 3, Line 446, Pos (0,0)
msgid ""
"\\>　　　　　　　とりあえず、ハッグよ。\n"
"\\>　　　　　　　今からギュッて抱きしめるから、\n"
"\\>　　　　　　　逃げないでね？"
msgstr ""

#. ID 1, Page 3, Line 455, Pos (0,0)
msgid "\\>　　　　　　　……温かいわね、あなたって。"
msgstr ""

#. ID 1, Page 3, Line 457, Pos (0,0)
msgid "\\>　　　　　　　う……うぅ……"
msgstr ""

#. ID 1, Page 3, Line 459, Pos (0,0)
msgid "\\>　　　　　　　……ごめんね。今まで、ずっと……"
msgstr ""

#. ID 1, Page 3, Line 461, Pos (0,0)
msgid "\\>　　　　　　　……ごめんね……"
msgstr ""

#. ID 1, Page 3, Line 465, Pos (0,0)
msgid "\\S[4] …………あ、あ……ぅ……"
msgstr ""

#. ID 1, Page 3, Line 481, Pos (0,0)
msgid "\\>　　　　　　　やはり、十分に想定し得た結果だわ。"
msgstr ""

#. ID 1, Page 3, Line 484, Pos (0,0)
msgid ""
"\\>　　　　　　　ま、いいでしょ。\n"
"\\>　　　　　　　今になって情が沸かないと\n"
"\\>　　　　　　　いえば嘘になるけど。"
msgstr ""

#. ID 1, Page 3, Line 487, Pos (0,0)
msgid ""
"\\>　　　　　　　所詮あなたは、\n"
"\\>　　　　　　　幼い日の私の火遊びが生んだ残滓。"
msgstr ""

#. ID 1, Page 3, Line 489, Pos (0,0)
msgid ""
"\\>　　　　　　　責任をもってあなたを始末し、\n"
"\\>　　　　　　　センターの前に墓標を立ててあげるわ。"
msgstr ""

#. ID 1, Page 3, Line 492, Pos (0,0)
msgid "\\>　　　　　　　私の、悔恨の思い出として。"
msgstr ""

#. ID 1, Page 3, Line 521, Pos (0,0)
msgid "\\> ………………！"
msgstr ""

#. ID 1, Page 3, Line 524, Pos (0,0)
msgid "\\> …………！"
msgstr ""

#. ID 1, Page 3, Line 526, Pos (0,0)
msgid "\\>\\S[6] ぁ……ああああぁぁ………………"
msgstr ""

#. ID 1, Page 5, Line 2, Pos (0,0)
msgid "\\>　　　　　　　そんな……この力は……"
msgstr ""

#. ID 1, Page 5, Line 3, Pos (0,0)
msgid ""
"\\>　　　　　　　あり得ない……\n"
"\\>　　　　　　　だって、それじゃ……"
msgstr ""

#. ID 1, Page 5, Line 17, Pos (0,0)
msgid "\\> …………あ。"
msgstr ""

#. ID 1, Page 5, Line 19, Pos (0,0)
msgid "\\> ……あ、あ、あ！"
msgstr ""

#. ID 1, Page 5, Line 21, Pos (0,0)
msgid "\\> 声が……声が。"
msgstr ""

#. ID 1, Page 5, Line 25, Pos (0,0)
msgid ""
"\\> ……もう、死んでるのよね。ソーニャ。\n"
"\\> せっかくお話しようと思ったのに。"
msgstr ""

#. ID 1, Page 5, Line 27, Pos (0,0)
msgid "\\> まあいいよ。勝手に話すからね。"
msgstr ""

#. ID 1, Page 5, Line 29, Pos (0,0)
msgid ""
"\\> どうしてあなたと、\n"
"\\> 私のパパを殺したかって。"
msgstr ""

#. ID 1, Page 5, Line 32, Pos (0,0)
msgid ""
"\\> フフフ、どうせ理解できない。\n"
"\\> 誰にもね。"
msgstr ""

#. ID 1, Page 5, Line 34, Pos (0,0)
msgid ""
"\\> パパも、あなたも、どいつもこいつも、\n"
"\\> 救いのないバカばっかりだから。"
msgstr ""

#. ID 1, Page 5, Line 37, Pos (0,0)
msgid ""
"\\> 私はね、歌を歌うことが\n"
"\\> 出来ればそれで良かったの。\n"
"\\> お金も、恋も、家族も、友達も、何も、\n"
"\\> 私には石ころと同じ。どうでもいいの。"
msgstr ""

#. ID 1, Page 5, Line 42, Pos (0,0)
msgid ""
"\\> 私は一度……\n"
"\\> どうしようもないクズどもに\n"
"\\> メチャクチャに陵辱されて、\n"
"\\> 殺されかけた。"
msgstr ""

#. ID 1, Page 5, Line 46, Pos (0,0)
msgid ""
"\\> このまま死ぬんだと、\n"
"\\> ただ冷静にそう考えたの。\n"
"\\> 別にそれでもいい、\n"
"\\> それが私の寿命だから。"
msgstr ""

#. ID 1, Page 5, Line 51, Pos (0,0)
msgid ""
"\\> でも、パパは私を生かした。\n"
"\\> 生体兵器として。\n"
"\\> ……私にはそれが許せなかった。"
msgstr ""

#. ID 1, Page 5, Line 54, Pos (0,0)
msgid ""
"\\> 声も出なくなるし、\n"
"\\> 頭もおかしくなってくる。\n"
"\\> 私にとってそれは、\n"
"\\> 生きてるということじゃない。"
msgstr ""

#. ID 1, Page 5, Line 58, Pos (0,0)
msgid ""
"\\> 死よりもなお暗く苦しい、\n"
"\\> 不快な灰色の世界。"
msgstr ""

#. ID 1, Page 5, Line 61, Pos (0,0)
msgid ""
"\\> あなたは。\n"
"\\> いまさら出てきて母親面？"
msgstr ""

#. ID 1, Page 5, Line 63, Pos (0,0)
msgid ""
"\\> 母親なんかじゃない。\n"
"\\> ウジ虫にも劣るただの売女。"
msgstr ""

#. ID 1, Page 5, Line 65, Pos (0,0)
msgid ""
"\\> 生み捨てたですって？\n"
"\\> お前のようなブタは\n"
"\\> 死んで当然よ、このクソ女。"
msgstr ""

#. ID 1, Page 5, Line 69, Pos (0,0)
msgid ""
"\\> 気にいらないものは全員殺せ。\n"
"\\> それはパパが私に言ったこと。\n"
"\\> 私はそうすることに決めたの。"
msgstr ""

#. ID 1, Page 5, Line 73, Pos (0,0)
msgid "\\> これからは好きに生きさせてもらうわ。"
msgstr ""

#. ID 1, Page 5, Line 74, Pos (0,0)
msgid "\\> 私は歌う。そして殺し続ける。"
msgstr ""

#. ID 1, Page 5, Line 75, Pos (0,0)
msgid "\\> 誰も私のことを止められないんですもの。"
msgstr ""

#. ID 1, Page 5, Line 77, Pos (0,0)
msgid ""
"\\> 世界は私を受け入れた。\n"
"\\> ……そういうことでしょ？"
msgstr ""

#. ID 1, Page 5, Line 81, Pos (0,0)
msgid "\\> Quand il me prend dans ses bras."
msgstr ""

#. ID 1, Page 5, Line 83, Pos (0,0)
msgid ""
"\\> Qu'il me parle tout bas.\n"
"\\> Je vois la vie en rose."
msgstr ""

#. ID 1, Page 5, Line 86, Pos (0,0)
msgid ""
"\\> Il me dit des mots d'amour.\n"
"\\> Des mots de tous les jours."
msgstr ""

#. ID 1, Page 5, Line 89, Pos (0,0)
msgid ""
"\\> Et ca m'fait quelque chose.\n"
"\\> Il est entre dans mon caur."
msgstr ""

#. ID 1, Page 5, Line 92, Pos (0,0)
msgid ""
"\\> Une part de bonheur.\n"
"\\> Dont je connais la cause..."
msgstr ""

